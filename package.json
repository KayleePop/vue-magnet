{
  "name": "vue-magnet",
  "version": "2.0.2",
  "description": "Adds a magnet attribute which acts like src, but supports torrent magnet links.",
  "main": "vue-magnet.js",
  "scripts": {
    "test": "standard && browserify -t vueify test/*.js | tape-run --static test && npm run test-other",
    "test-other": "browserify -t vueify test/separateInstance/*.js | tape-run --static test",
    "build": "browserify vue-magnet.js | minify > vue-magnet.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KayleePop/vue-magnet.git"
  },
  "keywords": [
    "vue",
    "plugin",
    "magnet",
    "link",
    "torrent",
    "webtorrent"
  ],
  "author": "KayleePop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KayleePop/vue-magnet/issues"
  },
  "dependencies": {
    "idbkv-chunk-store": "^1.0.2",
    "webtorrent": "^0.102.3"
  },
  "devDependencies": {
    "babel-minify": "^0.5.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^16.2.2",
    "md5-hex": "^2.0.0",
    "standard": "^12.0.0",
    "tape": "^4.9.1",
    "tape-run": "^4.0.0",
    "vue": "^2.5.16",
    "vueify": "^9.4.1"
  }
}
