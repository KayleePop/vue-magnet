{
  "name": "vue-magnet",
  "version": "1.2.0",
  "description": "Adds a magnet attribute which acts like src, but supports torrent magnet links.",
  "main": "vue-magnet.js",
  "scripts": {
    "test": "standard && browserify -t vueify test/*.js | tape-run && browserify -t vueify test/seperateTest/*.js | tape-run",
    "build": "browserify -t [ babelify --presets [ env ] ] vue-magnet.js | minify > vue-magnet.min.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KayleePop/vue-magnet.git"
  },
  "keywords": [
    "vue",
    "plugin",
    "magnet",
    "link",
    "torrent",
    "webtorrent"
  ],
  "author": "KayleePop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KayleePop/vue-magnet/issues"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "indexeddb-chunk-store": "^2.2.1",
    "webtorrent": "^0.98.24"
  },
  "devDependencies": {
    "babel-minify": "^0.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^16.0.0",
    "md5-hex": "^2.0.0",
    "standard": "^11.0.1",
    "tape": "^4.9.0",
    "tape-run": "^3.0.4",
    "vue": "^2.5.16",
    "vueify": "^9.4.1"
  }
}
